<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
 <default>
	<menu n="10">
		<label>SU-27SK</label>
		<enabled type="bool">true</enabled>
		<item>
			<label>Autostart toggle</label>
			<binding>
				<command>nasal</command>
				<script>
				misc.autostart();
				</script>
			</binding>
		</item>
		
		<item>
			<label>Toggle Ground Power</label>
		  <binding>
		   <command>property-toggle</command>
		   <property>controls/electric/external-power</property>
		  </binding>
			<binding>
				<command>nasal</command>
				<script>
					if (getprop("controls/electric/external-power") == 1){gui.popupTip("Ground power ON");}
					else {gui.popupTip("Ground power OFF");}
					</script>
			</binding>
		</item>
		
		<item>
			<label>RSBN Nav</label>
			<binding>
				<command>nasal</command>
				 <script>CanvasRSBN.showRSBN_Dlg()</script>
			</binding>
		</item>
		
		<item>
			<label>ARC Nav</label>
			<binding>
				<command>nasal</command>
				 <script>CanvasPU184.showRSBN_Dlg()</script>
			</binding>
		</item>
		
		<item>
			<label>Pylons loads</label>
			<binding>
				<command>nasal</command>
				 <script>CanvasPylonsDlg.showPylonsDlg()</script>
			</binding>
		</item>
		
		<item>
			<label>Aircraft help</label>
			<binding>
				<command>nasal</command>
				 <script>CanvasHelpDlg.showHelpDlg()</script>
			</binding>
		</item>
		
		<item>
			<label>Select Livery</label>
			<binding>
				<command>nasal</command>
				<script>aircraft.livery.dialog.toggle()</script>
			</binding>
		</item>

	</menu>
  </default>

</PropertyList>

